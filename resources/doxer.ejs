<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <link rel="stylesheet" media="all" href="doxer.css">
  </head>
  <body>
    <div id="container">
      <table cellpadding="0" cellspacing="0">
        <thead>
          <tr>
            <th class="docs">
              <h1>
                <%= title %>
              </h1>
            </th>
            <th class="code">
            </th>
          </tr>
        </thead>
        <tbody>
          <% for (var i = 0; i < sections.length; i++) { %>
            <tr id="section-<%= i + 1 %>">
              <td class="docs">
                <div class="pilwrap">
                  <a class="pilcrow" href="#section-<%= i + 1 %>">&#182;</a>
                </div>
                <%- sections[i].docs_html %>
              </td>
              <td class="code">
                <pre>    <%- sections[i].code_html %></pre>
              </td>
            </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  <body>
</html>
